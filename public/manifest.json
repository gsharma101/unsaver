{
  "manifest_version": 3,
  "name": "Unsaver for Instagram",
  "description": "Unsave Instagram posts and reels",
  "version": "5.0",
  "options_ui": {
    "page": "options.html"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "action": {
    "default_icon": "icon-128.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "downloads",
    "downloads.shelf",
    "cookies",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}