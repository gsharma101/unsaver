{
  "manifest_version": 3,

  "name": "Unsaver for Instagram",
  "description": "Unsave Instagram posts and stories",
  "version": "1.0",

  "options_ui": {
    "page": "options.html"
  },

  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
    "downloads",
    "cookies"
  ],

  "host_permissions": [
    "<all_urls>"
  ]
}
